{
  "name": "DailyTriggerCookForecastPipeline",
  "properties": {
    "type": "TumblingWindowTrigger",
    "runtimeState": "Started",
    "typeProperties": {
      "frequency": "Hour",
      "interval": "24",
      "startTime": "2018-01-07T04:00:00Z",
      "endTime": "2018-01-09T04:00:00Z",
      "maxConcurrency": 1
    },
    "pipeline": {
      "pipelineReference": {
        "type": "PipelineReference",
        "referenceName": "CookForecastPipeline"
      },
      "parameters": {
        "SliceStart": {
          "type": "Expression",
          "value": "@{formatDateTime(trigger().outputs.windowStartTime,'yyyyMMdd')}"
        }
      }
    }
  }
}